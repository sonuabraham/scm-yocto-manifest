<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <default sync-j="4" revision="honister"/>

  <remote fetch="git://git.yoctoproject.org" name="yocto"/>
  <remote name="freescale" fetch="git://github.com/Freescale"/>
  <remote fetch="ssh://git@bitbucket.org/rfiteam" name="git-rfi"/>
  <remote fetch="https://github.com/sonuabraham/" name="github"/> 


  <project remote="yocto"  upstream="honister" name="poky" path="sources/poky"/>
  <project name="meta-freescale" revision="refs/heads/honister" remote="yocto" path="sources/poky/meta-freescale" />
  <project name="scm-meta-rfi.git" path="sources/poky/meta-rfi" remote="git-rfi"
    revision="project/TRex-explicit-mode"/>
  <project name="scm-meta-rfi-dspbr-edge-scm.git"
	  path="sources/poky/meta-rfi-dspbr-edge-scm" remote="git-rfi"
	  revision="project/TRex-explicit-mode"/>

  <project remote="github" revision="master" name="rfi-bsp-base" path="sources/poky/base">
	<copyfile dest="README" src="README"/>
	<copyfile dest="setup-environment" src="setup-environment"/>
  </project>
</manifest>
